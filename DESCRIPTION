Package: factorstochvol
Type: Package
Title: Bayesian Estimation of (Sparse) Latent Factor Stochastic Volatility Models
Version: 1.0.4
Authors@R: c(
	person("Gregor", "Kastner", role = c("aut", "cre"), email = "gregor.kastner@aau.at", comment = c(ORCID = "0000-0002-8237-8271")),
